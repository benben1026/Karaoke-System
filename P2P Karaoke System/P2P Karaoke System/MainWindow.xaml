<Window x:Class="P2P_Karaoke_System.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="P2P Karaoke Player" Height="620" Width="800">
    <Grid Margin="0,0,-0.4,0.4" Background="#FF22243B">
        <Image x:Name="img" HorizontalAlignment="Left" Height="500" VerticalAlignment="Top" Width="500"/>
        <Button x:Name="Play" Content="Play" HorizontalAlignment="Left" Margin="186,521,0,0" VerticalAlignment="Top" Width="62" Height="46" FontSize="14" Click="Play_Click"/>
        <Button x:Name="Stop" Content="Stop" HorizontalAlignment="Left" Margin="280,521,0,0" VerticalAlignment="Top" Width="62" Height="46" FontSize="14"/>
        <Button x:Name="Next" Content="next" HorizontalAlignment="Left" Margin="365,521,0,0" VerticalAlignment="Top" Width="62" Height="46" FontSize="14"/>
        <Button x:Name="Previous" Content="previous" HorizontalAlignment="Left" Margin="94,521,0,0" Width="62" FontSize="14" Height="46" VerticalAlignment="Top"/>

    </Grid>
</Window>
